{
    "name": "rmzamora/user-bundle",
    "type": "symfony-bundle",
    "description": "Symfony SonataUserBundle rmzamora forked",
    "keywords": ["user", "sonata", "google authenticator", "rmzamora forked"],
    "homepage": "https://sonata-project.org/bundles/user",
    "license": "MIT",
    "authors": [
        {
            "name": "Mell M. Zamora",
            "email": "me@mellzamora.com",
            "homepage": "http://mellzamora.com"
        },
        {
            "name": "Thomas Rabaix",
            "email": "thomas.rabaix@sonata-project.org",
            "homepage": "https://sonata-project.org"
        },
        {
            "name": "Sonata Community",
            "homepage": "https://github.com/sonata-project/SonataUserBundle/contributors"
        }
    ],
    "require": {
        "symfony/http-foundation": "~2.4",
        "symfony/form": "~2.4",
        "symfony/security": "~2.4",
        "symfony/console": "~2.4",
        "sonata-project/core-bundle": "~2.2",
        "sonata-project/doctrine-extensions": "~1.0",
        "sonata-project/easy-extends-bundle": "~2.1",
        "sonata-project/google-authenticator": "~1.0",
        "sonata-project/datagrid-bundle": "~2.2",
        "friendsofsymfony/user-bundle": "~1.3",
        "rmzamora/admin-bundle": "2.4.2.*",
        "friendsofsymfony/rest-bundle": "~1.1",
        "jms/serializer-bundle": "~0.11|~1.0",
        "nelmio/api-doc-bundle": "~2.4"
    },
    "require-dev": {
        "doctrine/orm": "^2.2.3",
        "rmzamora/seo-bundle": "2.4.2.*",
        "symfony/phpunit-bridge": "~2.7|~3.0",
        "fabpot/php-cs-fixer": "~0.1|~1.0"
    },
    "conflict": {
        "jms/serializer": "<0.13"
    },
    "suggest": {
        "rmzamora/doctrine-orm-admin-bundle": "2.4.2.*",
        "sonata-project/seo-bundle": "~1.1"
    },
    "autoload": {
        "psr-4": { "Sonata\\UserBundle\\": "" }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-2.4.2": "2.4.2.x-dev"
        }
    }
}
